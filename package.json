    "eslint-config-standard-with-typescript": "40.0.0",
    "prisma": "5.6.0",
    "supertest": "7.0.0",
    "tsup": "8.0.1",
    "tsx": "4.4.0",
    "typescript": "5.3.2",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
  "dependencies": {
    "@fastify/jwt": "8.0.0",
